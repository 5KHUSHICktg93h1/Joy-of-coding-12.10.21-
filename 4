# Joy-of-coding-12.10.21-
#include<stdio.h> 
int main()
{
int a[20],b[20],n,i,j,count;
printf("Enter number of elements of array:\n"); 
scanf("%d",&n);
printf("Enter elements of array:\n"); for(i=0;i<n;i++)
{
scanf("%d",&a[i]);
b[i]=-1;
}
for(i=0;i<n;i++)
{
count=1; for(j=i+1;j<n;j++) {
if(a[i]==a[j]) {
count++;
b[j]=0; }
} if(b[i]!=0) b[i]=count;
} for(i=0;i<n;i++) {
if(b[i]!=0)
printf("Frequency of %d is %d\n",a[i],b[i]); }
return 0; }
