# Joy-of-coding-12.10.21-
#include<stdio.h> #include<stdlib.h> int main()
{
int a[20],i,n,j;
printf("Enter number of elements of array:\n"); 
scanf("%d",&n);
printf("Enter elements of array:\n"); 
for(i=0;i<n;i++)
scanf("%d",&a[i]);
for(i=0;i<n;i++)
{
for(j=i+1;j<n;j++) {
if(a[i]==a[j]) {
printf("Array is not unique");
exit(0); }
} }
printf("Array is unique");
return 0; 
}
